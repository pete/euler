: >numbers
	0 do dup dup c@ 0x30 - swap c! 1+ loop drop ;

: mul5
	1 5 0 do over i + c@ * loop nip ;

: 5max ( ns len -- max-product )
	swap 0 rot ( ns 0 len )
	4 - 0 do ( ns max )
		over i + mul5 max ( ns max' )
	loop ;

